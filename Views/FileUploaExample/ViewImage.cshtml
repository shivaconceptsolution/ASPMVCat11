@model IEnumerable<MVCExample.Models.tbl_upload>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ViewImage</title>
</head>
<body>
    <div> 
        @foreach(var item in Model)
        {

            <img src="~/img/@Html.DisplayFor(a=>item.imgname)" width="100" height="100" />
        }
    </div>
</body>
</html>
